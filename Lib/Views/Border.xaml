<?xml version="1.0" encoding="UTF-8"?>
<ContentView
	x:Class="HaFT.Xamarin.Forms.Lib.Border"
	xmlns="http://xamarin.com/schemas/2014/forms" 
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Name="B">

	<Grid x:Name="gOuter">
		<Grid x:Name="gInner">
			<ContentView Content="{Binding BorderContent,Source={x:Reference B}}" />
		</Grid>
	</Grid>
</ContentView>